{
  "expo": {
    "name": "Cyber Saathi",
    "slug": "cyber-saathi",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,


    "android": {
      "package": "com.cybersaathi.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 5,
      "splash": {
        "image": "./assets/images/logo.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      }
    },
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "91a09752-bf6f-49e1-8090-06430335466b"
      }
    }
  }
}
